{
    "name": "awesomite/var-dumper",
    "description": "The alternative for var_dump function",
    "license": "MIT",
    "keywords": ["dump", "var_dump", "debug", "var-dumper"],
    "authors": [
        {
            "name": "BartÅ‚omiej Krukowski",
            "email": "bartlomiej@krukowski.me"
        }
    ],
    "autoload": {
        "psr-4": {
            "Awesomite\\VarDumper\\": "src/"
        }
    },
    "autoload-dev": {
        "files": ["tests/polyfill.php"],
        "psr-4": {
            "Awesomite\\VarDumper\\": "tests/"
        }
    },
    "prefer-stable": true,
    "require": {
        "php": "^5.3 || ^7.0",
        "awesomite/iterators": "^1.0",
        "symfony/polyfill-mbstring": "^1.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8.36",
        "symfony/console": "^2.7 || ^3.0 || ^4.0",
        "sebastian/comparator": ">=1.2.3",
        "awesomite/phpunit-4.8-fixer": "^1.0",
        "symfony/var-dumper": "^2.7 || ^3.0 || ^4.0"
    },
    "conflict": {
        "symfony/var-dumper": "<2.7.16 || >=2.8.0 <2.8.9 || >=3.0.0 <3.0.9 || >=3.1.0 <3.1.3"
    },
    "suggest": {
        "symfony/var-dumper": "Required for Awesomite\\VarDumper\\SymfonyVarDumper"
    }
}
