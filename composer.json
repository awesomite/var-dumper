{
    "name": "awesomite/var-dumper",
    "description": "The alternative for var_dump function",
    "license": "MIT",
    "authors": [
        {
            "name": "BartÅ‚omiej Krukowski",
            "email": "bartlomiej@krukowski.me"
        }
    ],
    "autoload": {
        "psr-4": {
            "Awesomite\\VarDumper\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Awesomite\\VarDumper\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^5.3 || ^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8.35",
        "satooshi/php-coveralls": "^1.0.1 || ^2.0.0",
        "symfony/console": "^2.7 || ^3.0",
        "sebastian/comparator": ">=1.2.3"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.8-dev"
        }
    },
    "suggest": {
        "ext-mbstring": "For mb_str* functions.",
        "symfony/polyfill-mbstring": "When enabling `ext-mbstring` is not possible."
    }
}
